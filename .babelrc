{
  "presets": [["env", { "modules": false }], "react", "stage-2"],
  "plugins": [
    ["react-hot-loader/babel"]
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "test": {
      "presets": [
        ["env", {"modules": "commonjs"}],
        "react",
        "stage-2"
      ],
      "plugins": [
        "dynamic-import-node",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}